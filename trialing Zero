#include <iostream>
#include"bits/stdc++.h"
using namespace std;
#define ll long long

int findTrailingZeros(int n)
{
    // Initialize result
    int count = 0;
 
    
    for (int i = 5; n / i >= 1; i *= 5)
        count += n / i;
 
    return count;
}
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	ll t; cin>>t;
	while(t--)
	{
	   ll n;
	   cin>>n;
	   cout<<findTrailingZeros(n)<<endl;
	}
	return 0;
}
